# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  alica_capnzero_proxy_config = debug

else ifeq ($(config),release)
  alica_capnzero_proxy_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := alica_capnzero_proxy

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

alica_capnzero_proxy:
ifneq (,$(alica_capnzero_proxy_config))
	@echo "==== Building alica_capnzero_proxy ($(alica_capnzero_proxy_config)) ===="
	@${MAKE} --no-print-directory -C . -f alica_capnzero_proxy.make config=$(alica_capnzero_proxy_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f alica_capnzero_proxy.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   alica_capnzero_proxy"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"